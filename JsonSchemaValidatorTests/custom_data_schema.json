{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://checkout.com/custom_data.schema.json",
    "title": "Custom Data",
    "description": "Custom Data for additional payment information",
    "type": "object",
    "required": [],
    "additionalProperties": false,
    "properties": {
        "string_data": {
          "description": "The unique identifier for a product",
          "type": "string"
        },
        "integer_data": {
            "description": "The unique identifier for a product",
            "type": "integer"
        },
        "boolean_data": {
            "description": "The unique identifier for a product",
            "type": "boolean"
        },
        "array_data": {
            "description": "The unique identifier for a product",
            "type": "array",
            "items": {
                "type": "string"
              },
            "uniqueItems": true
        },
        "object_data": {
            "description": "The unique identifier for a product",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "nested_string_data": {
                  "type": "string"
                },
                "nested_number_data": {
                  "type": "number"
                },
                "nested_integer_data": {
                  "type": "integer"
                }
            }
        }
      }
  }